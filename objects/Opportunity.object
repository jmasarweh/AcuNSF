    <validationRules>
        <fullName>Closed_Lost_Reason_Required</fullName>
        <active>true</active>
        <description>If an opportunity is moved to closed/lost stage, an opportunity closed/lost reason is required.</description>
        <errorConditionFormula>AND ( ISPICKVAL(StageName, &quot;Closed Lost&quot;), ISBLANK( TEXT( Closed_Lost_Reason__c )) )</errorConditionFormula>
        <errorMessage>Closed Lost Reason Required When Stage is Closed/Lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_Reason_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( ISPICKVAL(StageName, &quot;Closed Won&quot;), ISBLANK( TEXT( Closed_Won_Reason__c )) )</errorConditionFormula>
        <errorMessage>Closed Won Reason Required When Stage is Closed/Won.</errorMessage>
    </validationRules>
