    <sharingModel>ReadWriteTransfer</sharingModel>
    <validationRules>
        <fullName>Lead_Conversion</fullName>
        <active>true</active>
        <description>Rule to ensure that a Lead can be converted only if the Lead has reached the Qualified stage.</description>
        <errorConditionFormula>AND(IsConverted,
    ISPICKVAL(Status,&quot;Qualified&quot;),
    OR( 
       ISBLANK(Lead_Source_Detail__c), 
       ISBLANK(FirstName), 
       ISBLANK(LastName), 
       ISBLANK(Company), 
       ISBLANK(Email), 
       ISBLANK(Street), 
       ISBLANK(City), 
       ISBLANK(State), 
       ISBLANK(Country), 
       ISBLANK(PostalCode) 
   ) 
)</errorConditionFormula>
        <errorMessage>The Lead must be moved to the Qualified status before it can be converted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Qualification</fullName>
        <active>true</active>
        <description>A Lead can be moved to the Qualified status only if the Lead Source, Lead Source Detail, Lead Name, Company and Address fields have been populated for the Lead.</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Status,&quot;Qualified&quot;),
  OR(
     ISBLANK(Lead_Source_Detail__c),
     ISBLANK(FirstName),
     ISBLANK(LastName),
     ISBLANK(Company),
     ISBLANK(Email),
     ISBLANK(Street),
     ISBLANK(City),
     ISBLANK(State),
     ISBLANK(Country),
     ISBLANK(PostalCode)
    )
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>A Lead cannot be Qualified unless the First Name, Last Name, Lead Source, Lead Source Detail, Company and full Address information has been identified for the Lead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Source_Pardot</fullName>
        <active>true</active>
        <description>Lead Source field is set to read-only if the Lead is coming from Pardot and Lead Source is set to the value &quot;Pardot&quot;.</description>
        <errorConditionFormula>AND(ISCHANGED(LeadSource),ISPICKVAL(PRIORVALUE(LeadSource),&quot;Pardot&quot;))</errorConditionFormula>
        <errorDisplayField>LeadSource</errorDisplayField>
        <errorMessage>Lead Source cannot be changed if Lead originated from Pardot and was created by Pardot in Salesforce.</errorMessage>
    </validationRules>
